graph G {
    node [fixedsize=true, shape=oval, width=0.7];

    A [label=Apple]
    B [label=Plum]
    C [label=Lime]
    D [label=Orange]
    E [label=Kiwi]
    F [label=Peach]

    A -- B;
    B -- C;
    A -- C;
    C -- D;
    A -- D;
    A -- E;
    D -- E;
    D -- F;
    C -- F;

    {rank=same; D, B}
    {rank=same; F, E}
}